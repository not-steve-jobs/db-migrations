{
  "name": "@internal/db-migrations",
  "version": "1.2.164",
  "description": "MW database migrations",
  "private": true,
  "license": "ISC",
  "engines": {
    "node": ">= 16.20.0",
    "npm": ">= 9.9.0",
    "yarn": "Please use NPM!"
  },
  "scripts": {
    "migrations:check": "ts-node src/bin/migrations-check.ts",
    "build": "npm install && rm -rf lib && tsc --build ./tsconfig.build.json",
    "clean": "rm -rf node_modules lib coverage",
    "db:recreate-counter-generator-schema": "ts-node src/bin/recreate-local-counter-generator.ts",
    "db:recreate-local-dev-schemas": "ts-node src/bin/recreate-and-seed.ts",
    "db:check-foreign-key-data-types": "node ./lib/bin/checkForeignKeyDataTypes.js",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "ci:code-style": "npm run lint",
    "ci:test": "npm run check:imports && npm run check:entities && tsc --build ./tsconfig.jest.json && npm run test",
    "ci:build": "tsc --build ./tsconfig.build.json",
    "ci:migrations:check": "node ./lib/bin/migrations-check.js",
    "typeorm": "src/custom-cli.js",
    "typeorm-extension": "src/custom-cli-extension.js",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "typeorm:migration:show": "npm run typeorm -- migration:show",
    "typeorm:migration:check": "npm run typeorm -- migration:generate src/migration/migration-name --check",
    "typeorm:migration:create": "src/custom-cli.js migration:create src/migration/your-migration",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate src/migration/migration-name",
    "typeorm:migration:revert": "npm run typeorm -- migration:revert",
    "typeorm:migration:fake-run": "npm run typeorm -- migration:run --fake",
    "typeorm:seed": "src/custom-cli-extension.js seed:run",
    "clean:build": "rm -rf lib coverage",
    "typeorm:test-cli": "DB_TEST=1 node ./lib/custom-cli.js",
    "typeorm:test-cli-extension": "DB_TEST=1 node ./lib/custom-cli-extension.js",
    "typeorm:test:setup": "npm run typeorm:test-cli-extension -- db:drop && npm run typeorm:test-cli-extension -- db:create && npm run typeorm:test:migrate",
    "typeorm:test:migrate": "npm run typeorm:test-cli -- migration:run",
    "typeorm:test:revert": "npm run typeorm:test-cli -- migration:revert",
    "typeorm:test:seed": "npm run typeorm:test-cli-extension -- seed:run",
    "typeorm:test:migration:check": "npm run typeorm:test-cli -- migration:generate src/migration/migration-name --check",
    "typeorm:test:check-foreign-key-data-types": "DB_TEST=1 node ./lib/bin/checkForeignKeyDataTypes.js",
    "check:entities": "bash ./check-entities.sh",
    "check:imports": "bash ./check-imports.sh",
    "test": "npm run typeorm:test:setup && npm run typeorm:test:revert && npm run typeorm:test:migrate && npm run typeorm:test:migration:check && npm run typeorm:test:check-foreign-key-data-types && npm run typeorm:test:seed && npm run test:unit",
    "test:clear-cache": "jest --clearCache",
    "test:watch": "jest --watch",
    "test:unit": "jest --runInBand --ci --logHeapUsage --no-cache --detectOpenHandles --coverage"
  },
  "dependencies": {
    "@internal/config-library": "1.0.1",
    "commander": "^11.0.0",
    "mysql2": "^3.6.1",
    "shelljs": "^0.8.5",
    "strip-comments": "^2.0.1",
    "ts-morph": "^20.0.0",
    "ts-node": "10.7.0",
    "typeorm": "^0.3.17",
    "typeorm-extension": "3.4.0",
    "typescript": "4.6.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@internal/chassis-cli": "^0.0.81",
    "@internal/eslint-config-mw-development": "1.0.9",
    "@internal/jest-extends": "0.0.5",
    "@tsconfig/node16": "16.1.3",
    "@types/jest": "29.5.11",
    "@types/lodash.pick": "^4.4.7",
    "@types/shelljs": "^0.8.12",
    "@types/strip-comments": "^2.0.4",
    "@types/uuid": "^9.0.2",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "4.7.4"
  },
  "files": [
    "lib",
    "assets",
    "src/migration",
    "tsconfig.json"
  ],
  "bin": {
    "db-migrations-recreate-and-seed": "lib/bin/recreate-and-seed.js",
    "db-migrations-recreate-local-counter-generator": "lib/bin/recreate-local-counter-generator.js",
    "db-migrations-check-foreign-key-data-types": "lib/bin/checkForeignKeyDataTypes.js",
    "typeorm-cli": "lib/custom-cli.js",
    "typeorm-cli-extension": "lib/custom-cli-extension.js",
    "migrations-check": "lib/bin/migrations-check.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.fxpro.local:middleware/utilities/db-migrations.git"
  }
}
